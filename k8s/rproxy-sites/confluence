server {
	listen 443 ssl;

	root /var/www/html;
	
	server_name confluence.sdte.dev;

	# Add index.php to the list if you are using PHP
	index indexjira.html;

	ssl_certificate		/etc/ssl/sdte/confluence.sdte.dev/confluence.sdte.dev-chain.pem;
	ssl_certificate_key	/etc/ssl/sdte/confluence.sdte.dev/confluence.sdte.dev.key;

	if ($scheme != "https") {
		return 301 https://$host$request_uri;
	}
	#ssl_protocols	TLSv1 TLSv1.1 TLSv1.2
	#ssl_ciphers HIGH:!aNULL:!MD5

	location / {
