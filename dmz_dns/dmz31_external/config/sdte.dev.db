sdte.dev. IN SOA dns.sdte.dev. hostmaster.sdte.dev 2021020112 7200 3600 1209600 3600 ; SOA

@ IN TXT "This is a CoreDNS [EXTERNAL] for sdte.dev v0.5.1" ;

;SDTE Public Access
vpn.sdte.dev.		IN A 144.139.116.59;
sdte.dev.		IN A 144.139.116.59;
orphan.sdte.dev.	IN A 59.167.63.236;

;SDTE Externally Available Domains
keycloak.sdte.dev.	IN CNAME sdte.dev.
jira.sdte.dev.		IN CNAME sdte.dev.
confluence.sdte.dev.	IN CNAME sdte.dev.
;SDTE Public Nameservers
ns1.sdte.dev.		IN A 144.139.116.59;
ns2.sdte.dev.		IN A 144.139.116.59;

sdte.dev.		IN NS ns1.sdte.dev.;
sdte.dev.		IN NS ns2.sdte.dev.;

;JCOMS Public Access
dco.sdte.dev. 		IN CNAME dcolab.ddns.net.;
*.dco.sdte.dev. 	IN CNAME dco.sdte.dev.;


;EMAIL records
;MX Records. Although no incoming mail (yet) SPAMASSASIN likes to see them apparently

sdte.dev.		IN MX 10 nomail.sdte.dev. ;
;Fake A record for nomail
nomail.sdte.dev.	IN A 144.139.116.59;
;SPF
sdte.dev.		IN TXT "v=spf1 a -all";
;DMARC
_dmarc.sdte.dev.	IN TXT "v=DMARC1; p=none";
;DKIM
_dkim._domainkey.sdte.dev. IN TXT "v=DKIM1; k=rsa; "

